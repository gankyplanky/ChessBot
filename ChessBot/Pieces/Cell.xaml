<UserControl x:Class="ChessBot.Pieces.Cell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChessBot.Pieces"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid x:Name="cellContainer" MouseLeftButtonDown="cellContainer_MouseLeftButtonDown" Background="Transparent">
        <Image x:Name="moveIndicator" Source="../Resources/Images/moveIndicator.png" Visibility="Collapsed"/>
    </Grid>
</UserControl>
